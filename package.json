{
  "name": "vue-vite-template",
  "version": "0.0.1",
  "private": true,
  "main": "dist-electron/electron-main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "electron:dev": "vite --config vite.config.electron.ts",
    "electron:build": "vue-tsc && vite build --config vite.config.electron.ts && electron-builder",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@seald-io/nedb": "^4.1.1",
    "electron-log": "^4.4.8",
    "electron-updater": "^6.6.2"
  },
  "devDependencies": {
    "@electron/remote": "^2.1.2",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@ionic/vue": "^8.7.3",
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.15.23",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^22.3.27",
    "electron-builder": "^26.0.12",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "pinia": "^2.3.1",
    "prettier": "3.5.3",
    "sass-embedded": "^1.80.2",
    "sudo-prompt": "^9.2.1",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-bundle-obfuscator": "^1.7.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.4",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.8"
  }
}
